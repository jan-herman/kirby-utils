{
    "name": "jan-herman/kirby-utils",
    "description": "Collection of utilities for Kirby CMS.",
    "version": "2.7.0",
    "type": "kirby-plugin",
    "license": "MIT",
    "homepage": "https://github.com/jan-herman/kirby-utils",
    "authors": [
        {
            "name": "Jan Herman",
            "email": "mail@janherman.com",
            "homepage": "https://janherman.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "getkirby/composer-installer": "^1.2"
    },
    "extra": {
        "installer-name": "utils"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "JanHerman\\Utils\\": "lib/"
        }
    }
}